    public class PatientTileOnScreenPayload {
        @AuraEnabled  public Map<String,String> labelApiMapForSelectedEntity{get;set;}
    }
    public class PatientTileWrappper {
        @AuraEnabled  public List<ElixirSuite__Master_Object__c> allpatientTileRecord{get;set;}
        @AuraEnabled  public List<ElixirSuite__Master_Object__c> allUserLocRecord{get;set;}
        @AuraEnabled  public Map<String,String> labelApiMapForSelectedEntity{get;set;}
    }
    /**************** QUERY DUMPING GROUND ************************/
    public static List<ElixirSuite__Master_Object__c> fetchAllPatientTileRecordForGivenEntity(String entity){
         Id recordTypeId =
            Schema.SObjectType.ElixirSuite__Master_Object__c.getRecordTypeInfosByDeveloperName()
            .get('Patient_Info_Card_Configuration').getRecordTypeId();
        return [SELECT ElixirSuite__Patient_Tile_Entity__c,ElixirSuite__Patient_Tile_Order__c,ElixirSuite__Field_Api__c,ElixirSuite__Field_Label__c
                FROM ElixirSuite__Master_Object__c
                WHERE RecordTypeId=:recordTypeId and ElixirSuite__Patient_Tile_Entity__c =: entity WITH SECURITY_ENFORCED ORDER BY CreatedDate DESC];
    }
