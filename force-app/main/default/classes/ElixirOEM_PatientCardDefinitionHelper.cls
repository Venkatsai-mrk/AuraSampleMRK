public with sharing class ElixirOEM_PatientCardDefinitionHelper {
    @AuraEnabled
    public static PatientTileOnScreenPayload fetchOptions_SetupKey(String setUpKey) {
        PatientTileOnScreenPayload toReturn = new PatientTileOnScreenPayload();
        toReturn.labelApiMapForSelectedEntity = ElixirOEM_FormConfigurationHelper.fetchFieldsForSelectedObject(setUpKey);
        return toReturn;
    }
    @AuraEnabled
    public static void saveLocationForUser(String patientTileConfigData,String entity) {
        Id recordTypeId =
            Schema.SObjectType.ElixirSuite__Master_Object__c.getRecordTypeInfosByDeveloperName()
            .get('Patient_Info_Card_Configuration').getRecordTypeId();
        Map<String, Object> mapOfPatientTileData =(Map<String, Object>) JSON.deserializeUntyped(patientTileConfigData);
        List<Object> obj_patientTileData = (List<Object>) mapOfPatientTileData.get('patientTileDataToSave');
        SYSTEM.debug('obj_patientTileData '+obj_patientTileData);
        ElixirSuite__Master_Object__c[] toInsertArr = new List<ElixirSuite__Master_Object__c>();
        for(Object sObjRec : obj_patientTileData){
            ElixirSuite__Master_Object__c objToInsert = new ElixirSuite__Master_Object__c();
            Map<String, Object> mapOfPatientTileConfigData = (Map<String, Object>) sObjRec;   
            objToInsert.ElixirSuite__Field_Label__c = String.valueOf(mapOfPatientTileConfigData.get('label'));
            objToInsert.ElixirSuite__Field_Api__c =  String.valueOf(mapOfPatientTileConfigData.get('value'));
            objToInsert.ElixirSuite__Patient_Tile_Entity__c = entity;
            objToInsert.ElixirSuite__Patient_Tile_Order__c =  Integer.valueOf(mapOfPatientTileConfigData.get('order'));
            objToInsert.RecordTypeId = recordTypeId;
            toInsertArr.add(objToInsert);
        }
        DMLManager.insertAsUser(toInsertArr);
        
    }
    @AuraEnabled
    public static void updateLocation(String patientTileConfigData,String entity,Boolean delAll) {
        Id recordTypeId =
                Schema.SObjectType.ElixirSuite__Master_Object__c.getRecordTypeInfosByDeveloperName()
                .get('Patient_Info_Card_Configuration').getRecordTypeId();
        if(delAll){
            delete [SELECT ID FROM ElixirSuite__Master_Object__c where ElixirSuite__Patient_Tile_Entity__c=:entity 
                    AND recordTypeId = : recordTypeId];
        }
        else {
            
            delete [SELECT ID FROM ElixirSuite__Master_Object__c where ElixirSuite__Patient_Tile_Entity__c=:entity 
                    AND recordTypeId = : recordTypeId];
            saveLocationForUser(patientTileConfigData,entity);
        }
        
    }
    @AuraEnabled
    public static PatientTileWrappper fetchExistingLocationRecordForGivenEntity(String entityName){
        PatientTileWrappper recordArray = new PatientTileWrappper();
        recordArray.allpatientTileRecord = fetchAllPatientTileRecordForGivenEntity(entityName);
        recordArray.allUserLocRecord = fetchAllPatientTileRecordForGivenEntity(entityName);
        recordArray.labelApiMapForSelectedEntity = ElixirOEM_FormConfigurationHelper.fetchFieldsForSelectedObject(entityName);
        return recordArray;
    }
