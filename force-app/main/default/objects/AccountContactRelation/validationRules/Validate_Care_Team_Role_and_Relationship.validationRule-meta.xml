<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Care_Team_Role_and_Relationship</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(TEXT(Care_Team_Role__c))),
NOT(ISBLANK(TEXT(Relationship__c)))
)</errorConditionFormula>
    <errorMessage>You cannot have values in both Relationship and Care Team Role fields</errorMessage>
</ValidationRule>
