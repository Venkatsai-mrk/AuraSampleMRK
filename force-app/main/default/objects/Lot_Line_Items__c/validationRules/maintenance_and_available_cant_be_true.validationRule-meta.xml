<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>maintenance_and_available_cant_be_true</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    In_Maintenance__c,
    Available__c
)</errorConditionFormula>
    <errorMessage>Available and In maintenance cannot be true at the same time.</errorMessage>
</ValidationRule>
