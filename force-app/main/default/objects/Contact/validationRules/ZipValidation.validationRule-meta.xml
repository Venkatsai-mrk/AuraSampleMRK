<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ZipValidation</fullName>
    <active>false</active>
    <errorConditionFormula>If(LEN( MailingPostalCode )=5 || LEN( MailingPostalCode )=9,false,true) &amp;&amp;  ISPICKVAL(ElixirSuite__Type__c, &apos;Guarantor&apos;)</errorConditionFormula>
    <errorDisplayField>MailingPostalCode</errorDisplayField>
    <errorMessage>Zip code size should be 5 or 9</errorMessage>
</ValidationRule>
