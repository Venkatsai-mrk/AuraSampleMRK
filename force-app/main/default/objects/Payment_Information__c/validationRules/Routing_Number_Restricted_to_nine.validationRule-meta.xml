<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Routing_Number_Restricted_to_nine</fullName>
    <active>true</active>
    <errorConditionFormula>IF(AND(NOT(Isblank(Routing_Number__c)),

OR(LEN(Routing_Number__c)&gt;9,LEN(Routing_Number__c)&lt;9)),true,false)</errorConditionFormula>
    <errorMessage>Routing Number Must be of 9 digit&apos;s</errorMessage>
</ValidationRule>
