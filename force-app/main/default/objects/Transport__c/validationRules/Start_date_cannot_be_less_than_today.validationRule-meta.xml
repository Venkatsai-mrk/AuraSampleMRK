<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_date_cannot_be_less_than_today</fullName>
    <active>false</active>
    <errorConditionFormula>AND(BypassValidation__c=FALSE,Start_Time__c &lt; NOW())</errorConditionFormula>
    <errorDisplayField>Start_Time__c</errorDisplayField>
    <errorMessage>Start date cannot be less than today.</errorMessage>
</ValidationRule>
