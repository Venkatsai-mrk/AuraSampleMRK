<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_date_cannot_be_greater_than_today</fullName>
    <active>true</active>
    <errorConditionFormula>AND(BypassValidation__c=FALSE,DATETIMEVALUE(Start_Time__c ) &gt; (NOW()))</errorConditionFormula>
    <errorDisplayField>Start_Time__c</errorDisplayField>
    <errorMessage>Start date cannot be greater than today.</errorMessage>
</ValidationRule>
