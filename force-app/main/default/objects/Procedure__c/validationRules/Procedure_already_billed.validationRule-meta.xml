<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Procedure_already_billed</fullName>
    <active>false</active>
    <errorConditionFormula>AND(Is_Billable__c  ==true &amp;&amp; ISCHANGED(Ready_for_Billing__c) &amp;&amp; NOT(ISNEW()))</errorConditionFormula>
    <errorMessage>Procedure is already billed.</errorMessage>
</ValidationRule>
