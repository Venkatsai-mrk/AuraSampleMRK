<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Procedure_Not_Billable</fullName>
    <active>false</active>
    <errorConditionFormula>AND(Is_Billable__c  == true,NOT(ISNEW()),ISCHANGED(Is_Billable__c))</errorConditionFormula>
    <errorMessage>Procedure is not billable.</errorMessage>
</ValidationRule>
