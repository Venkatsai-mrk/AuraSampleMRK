<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"
                controller="CMS1500ClaimFormApex" access="global" >
    <lightning:workspaceAPI aura:id="workspace"/>
    <aura:registerEvent name="ProblemRefreshEvt" type="c:ProblemRefreshEvt"/>
    <aura:attribute name="options" type="List" default="[
                                                        {'label': 'Yes', 'value': 'option1'},
                                                        {'label': 'No', 'value': 'option2'}
                                                        ]"/>
    
    <aura:attribute name="options1" type="List" default="[
                                                         {'label': 'SSN', 'value': 'option1'},
                                                         {'label': 'EIN', 'value': 'option2'}
                                                         ]"/>
    
    <aura:attribute name="selectedVOBList" type="list" />
    <aura:attribute name="value" type="List"/>
    <aura:attribute name="value1" type="List"/> 
    
    <aura:attribute name="careBillingProviderCMS" type="Object" default=""/>
    <aura:attribute name="careServiceProviderCMS" type="Object" default=""/>
    <aura:attribute name="careRenderingProviderCMS" type="Object" default=""/>
    <aura:attribute name = "preAuthNumCMS" type = "String" default=""/>
    <aura:attribute name = "careIdCMS" type = "String" default=""/>
    
    <aura:attribute name = "isCMS1500" type = "Boolean" default = "false"/>
    <aura:attribute name = "isAutoAccident" type = "String" />
    <aura:attribute name = "isEmployement" type = "String" />
    <aura:attribute name = "isOtherAccident" type = "String" />
    <aura:attribute name = "heading" type = "String"/>
    <aura:attribute name = "subheading" type = "String"/>
    <aura:attribute name="recordTypeId" type="String"/>
    <aura:attribute name="isActive" type="Boolean"/>
    <aura:attribute name="plugData" type="boolean" default="False"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/> 
    <aura:attribute name="OpenPopUp" type="Boolean" default="true"/>
    <aura:attribute name="insuranceTypePicklistValues" type="List"/>
    <aura:attribute name="medicareTypeCodePicklistValues" type="List"/>
    <aura:attribute name="relationshipTypePicklistValues" type="List"/>
    <aura:attribute name="employmentPicklistValues" type="List"/>
    <aura:attribute name="autoAccidentPicklistValues" type="List"/>
    <aura:attribute name="otherAccidentPicklistValues" type="List"/>
    <aura:attribute name="otherHealthBenefitPicklistValues" type="List"/>
    <aura:attribute name="genderPicklistValues" type="List"/>
    <aura:attribute name="InsuregenderPicklistValues" type="List"/>
    <aura:attribute name="AccptAssPicklistValues" type="List"/>
    <aura:attribute name="federalPicklistValues" type="List"/>
    <aura:attribute name = "commaSeparInsudName" type = "String"/>
    <aura:attribute name="typeOfClaim" type="List"/>
    <aura:attribute name="patId" type="Id"/>
    <aura:attribute name="jsonList" type="List"/>  
    <aura:attribute name="transactionaljsonList" type="List"/>
    <aura:attribute name="transactionalDataId" type="String"/>
    <aura:attribute name="messege" type="Boolean" default= "true"/>
    <aura:attribute name="totalAmount" type="String"/>    
    <aura:attribute name="fromDate" type="String"/>
    <aura:attribute name="toDate" type="String"/>
    <aura:attribute name="secondvalidation" type="Boolean" default="false"/>
    <aura:attribute name="thirdvalidation" type="Boolean" default="false"/>
    <aura:attribute name="fourthvalidation" type="Boolean" default="false"/>
    <aura:attribute name="firstvalidation" type="Boolean" default="false"/>
    <aura:attribute name="claimProcessedVia" type="Boolean" default="false"/>
    <aura:attribute name="allData" type="List"/>
    <aura:attribute name="allDataCopy" type="List"/>
    <aura:attribute name="newData" type="Claim__c"/>
    <aura:attribute name="commaSeparatedName" type="String"/>
    <aura:attribute name="payorName" type="String" default=""/> <!-- used to store insurance payor id-->
    <aura:attribute name="isSetUpKeySelected" type="Boolean" default="false" />
    <aura:attribute name="searchString"   type="string" access="private" default="" description="String to search"/>
    <aura:attribute name="isdisAbled" type="Boolean"/>
    <aura:attribute name="dropDownOptions" type="List"/>
    <aura:attribute name="dropDownSelectedValues" type="List"/>
    <aura:attribute name="dropdownLabel" type="String"/>
    
    <aura:attribute name="enableSecInsurance" type="Boolean" default="false"/>
    
    <!--MY ATTRIBUTES-->
    <aura:attribute name="qualifierForDateOfCurrentIllness" type="List"/>
    <aura:attribute name="qualifierForOtherDates" type="List"/>
    <aura:attribute name="npiValues" type="List"/> 
    <aura:attribute name="isInpatient" type="Boolean" default="false"/>
    <aura:attribute name="outsideLabValues" type="List"/> 
    <aura:attribute name="referringProviderQualifier" type="List"/>
    <aura:attribute name="qualifierForProcedure" type="List"/>
    <aura:attribute name="espdtValues" type="List"/>
    <aura:attribute name="chargeAbility" type="Boolean"/> 
    <aura:attribute name="loaded" type="Boolean" default="false" />
    <aura:attribute name="table_FromDateValid" type="Boolean" default="false" />
    <aura:attribute name="table_ToDateValid" type="Boolean" default="false" />
    <aura:attribute name="searchRecords" type="List"/>
    <aura:attribute name="emptiedDiagnosisBoxes" type="List"/>
    <aura:attribute name="selectRecordId" type="String"/> 
    <aura:attribute name="lastPointerFilled" type="String" default="0"/> 
    <aura:attribute name="alreadyExistingICDCode" type="List"/>
    <aura:attribute name="placeOfServiceArray" type="List"/>
    <aura:attribute name="fetchedRowIndexArray" type="List"/>
    <aura:attribute name="alreadyexistingProcedure" type="List"/>
    <aura:attribute name="mapOfPointerAndICD" type="Object"/>
    <aura:attribute name="mapOfchildIndexAndFilledPointers" type="Object"/>
    <aura:attribute name="isAddClaimEnabled" type="Boolean"/> 
    <aura:attribute name="autoAccPlaceOrStateDisabled" type="Boolean" default="true"/> 
    <aura:attribute name="existingProcedurePrePopulated" type="List"/>
    <aura:attribute name="attachedProcId" type="List"/>
    <aura:attribute name="StatesPicklistVal" type="List"/>
    <aura:attribute name="showConfirmation" type="Boolean" default="false" />
    <aura:attribute name="errorMessage" type="string"/>
    <aura:attribute name = "parentindexValue" type = "String" default=""/>
    <aura:attribute name = "childindexValue" type = "String" default=""/>
    <aura:attribute name="StatesAndItsShotforms" type="Object"/> <!--Anusha 07/11/22-->
    <aura:registerEvent name="cancelEvent" type="c:closePopUp"/> <!--Anusha 29/10/22 -->
    <lightning:navigation aura:id="navigationService" />
    <!--END-->
    <aura:if isTrue="{!v.isActive}">
        <div aura:id="exampleModal" class="cms_box slds-align_absolute-center">
            <div class="slds-m-around_xx-large">
                <div  role="dialog" tabindex="-1" aria-hidden="false" aria-labelledby="modal-heading-01"  aria-modal="true" aria-describedby="modal-content-id-1"  class="slds-modal slds-fade-in-open" >
                    <div class="slds-modal__container">                        
                        <header class="slds-modal__header">
                            <button class="slds-button slds-button--icon-inverse slds-modal__close" onclick="{!c.cancelButton}">
                                <lightning:icon iconName="utility:close" size="medium" variant="bare"/>
                                <span class="slds-assistive-text">Close</span>
                            </button>
                            
                            <div class="slds-grid slds-wrap res_goal-form">
                                <div class="slds-size_12-of-12 slds-p-top_medium">
                                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate changeFont colPayHeader"><b>{!v.heading}</b></h2>                            
                                </div>
                            </div> 
                        </header>
                        <lightning:spinner variant="brand" size="large" aura:id="Id_spinner" class="slds-hide" />
                        
                        <div class="slds-modal__content slds-p-around_small" id="modal-content-id-1">
                            <div>
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-col slds-size_12-of-12 heading slds-m-bottom_medium">
                                        <h2 class="slds-text-heading_large">HEALTH INSURANCE CLAIM FORM</h2>
                                        <p>APPROVED BY NATIONAL UNIFORM CLAIM COMMITTEE (NUCC)</p>
                                    </div>
                                </div>
                            </div>
                            <!-- <div class="slds-col slds-size_12-of-12">
                                    
                                    <div class="slds-section slds-is-open" aura:id="task2">
                                        <h3 class="slds-section__title">
                                            <button aria-controls="TaskList2" class="slds-button slds-section__title-action">
                                                <span onclick="{!c.toggleSection}" data-auraId="task2" >
                                                    <lightning:icon iconName="utility:switch"
                                                                    size="x-small"
                                                                    class="slds-section__title-action-icon slds-button__icon_left"
                                                                    alternativeText="button icon" 
                                                                    />
                                                </span>
                                                <span class="slds-truncate" title="tapers">PATIENT AND INSURED INFORMATION</span>
                                            </button>
                                        </h3>-->
                            <aura:iteration items="{!v.jsonList}" var="sample" indexVar="idx1">
                                <aura:iteration items="{!sample.Record}" var="allData" indexVar="idx">
                                    <div class="cms_box slds-p-around_small slds-m-bottom_small">
                                        <div class="slds-grid slds-gutters">
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">1. Insurance Type
                                                <lightning:select aura:id="checkone" name="{!allData.insuranceType + '$ElixirSuite__Insurance_Type__c'}" label="{!allData.insuranceType}" value="{!allData.insuranceType}" onchange="{!c.radioCheckOne}">
                                                    <aura:iteration items="{!v.insuranceTypePicklistValues}" var="optionItem">
                                                        <option value="{!optionItem.value}" label="{!optionItem.label}" selected="{!allData.insuranceType == optionItem.value}"/>
                                                    </aura:iteration>
                                                </lightning:select>
                                            </div>
                                            <aura:if isTrue="{!allData.insuranceType == 'MA' || allData.insuranceType == 'MB'}">
                                                <div class="slds-col slds-size_4-of-12 slds-m-bottom_small"> Medicare Type Code
                                                    <lightning:select aura:id="checkMed" name="{!allData.medicareTypeCode + '$ElixirSuite__Medicare_Type_Code__c'}" label="{!allData.medicareTypeCode}" value="{!allData.medicareTypeCode}">
                                                        <aura:iteration items="{!v.medicareTypeCodePicklistValues}" var="optionItem">
                                                            <option value="{!optionItem.value}" label="{!optionItem.label}" selected="{!allData.medicareTypeCode == optionItem.value}"/>
                                                        </aura:iteration>
                                                    </lightning:select>
                                                </div>
                                            </aura:if>
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">1a. INSURED'S ID NUMBER
                                                <lightning:helptext content="The Insured’s ID Number is the identification number of the insured. This information identifies the insured to the payer. This field allows for entry of 29 characters." class="customIcon"/>
                                                <lightning:input type="text" requiered="true" value="{!allData.insuredIdNumber}" onblur="{!c.isDataFilled}" maxlength="32"
                                                                 name="{!idx1+'$'+idx}" aura:id="insuredNumber"/>  <!--Anuhsa - Modified max length 29 to 32-->
                                            </div>
                                        </div>
                                        <div class="slds-grid slds-gutters">
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">2. PATIENT'S NAME (Last Name, First Name, Middle Name) <span class="mendate-field">*</span>
                                                <lightning:helptext content="Use commas to separate the last name, first name, and middle initial." class="customIcon"/>
                                                <lightning:input type="text" value="{!v.commaSeparatedName}" required="true"  maxlength="28"/>
                                                
                                            </div>
                                            <div class="slds-col slds-size_2-of-12 slds-m-bottom_small">3. PATIENT'S BIRTH DATE <span class="mendate-field">*</span> 
                                                <lightning:input type="date" value="{!allData.patientDOB}"  required="true" aura:id="globalReqCheck" dateStyle="short"/>
                                            </div>
                                            
                                            <div class="slds-col slds-size_2-of-12 slds-m-bottom_small">SEX       
                                                <div class="slds-grid slds-gutters check_row"> 
                                                    <!--<lightning:input type="text" value="{!allData.patiSex}" />-->
                                                    <aura:iteration items="{!v.genderPicklistValues}" var="item">
                                                        <div class="slds-col slds-size_2-of-6 slds-text-align_left">
                                                            <lightning:input type="checkbox" aura:id="checkTwo" name="{!item + '$ElixirSuite__Gender__c'}" 
                                                                             label="{!item}" checked="{!allData.patiSex == item}" onchange="{!c.radioCheckTwo}"/>
                                                        </div>
                                                    </aura:iteration>
                                                </div>
                                            </div>
                                            
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">4. INSURED'S NAME (Last Name, First Name, Middle Name)
                                                <lightning:helptext content="Enter the insured’s full last name, first name, and middle initial. If the insured uses a last name suffix (e.g., Jr, Sr),  enter it after the last name and before the first name.  Titles (e.g., Sister, Capt, Dr) and professional suffixes (e.g., PhD, MD, Esq) should not be included with the name.Use commas to separate the last name, first name, and middle initial. A hyphen can be used for hyphenated names. Do not use periods within the name." class="customIcon"/>
                                                <lightning:input type="text" value="{!v.commaSeparInsudName}"  maxlength="29" onchange="{!c.inputInsName}"/>
                                            </div>
                                        </div>
                                        
                                        <div class="slds-grid slds-gutters">
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12 slds-m-bottom_x-small">5. PATIENT'S ADDRESS(No., Street)
                                                        <lightning:helptext content="Do not use punctuation (i.e., commas, periods) or other symbols in the address." class="customIcon"/>
                                                        <lightning:textarea  type="text" value="{!allData.patientAddress}"  name="{!idx1+'$'+idx}" aura:id="patientAdress"  
                                                                            onblur="{!c.validateSpecialCharactersForAdress}"/>
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_6-of-12 slds-m-bottom_x-small">CITY <span class="mendate-field">*</span>
                                                        <lightning:input type="text" value="{!allData.patientCity}" maxlength="24" required="true" aura:id="globalReqCheck" />
                                                    </div>
                                                    <div class="slds-col slds-size_6-of-12 slds-m-bottom_x-small">STATE <span class="mendate-field">*</span>
                                                        <lightning:input type="text" value="{!allData.patientState}" maxlength="2" required="true" aura:id="globalReqCheck" />
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_6-of-12 slds-m-bottom_x-small">ZIP CODE
                                                        <lightning:input type="text" value="{!allData.patientZipcode}" maxlength="15" name="{!idx1+'$'+idx}"  
                                                                         onblur="{!c.validatePatientZip}" aura:id="patientzip"/> <!--Anuhsa - Modified max length 12 to 15-->
                                                    </div>
                                                    <div class="slds-col slds-size_6-of-12 slds-m-bottom_x-small">TELEPHONE(Include Area Code)
                                                        <lightning:input type="PhoneNumber" value="{!allData.patientPhone}" maxlength="10" name="{!idx1+'$'+idx}"
                                                                         onblur="{!c.validatePhoneNumber}" aura:id="patientPhoneNumber_Patient"/>  <!--Anuhsa - Modified max length 13 to 10-->
                                                    </div>
                                                </div>
<div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small"> PATIENT'S ADDRESS 2
                                                        <lightning:textarea type="textarea" value="{!allData.patientAddress2}" maxlength="29"/>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12 slds-m-bottom_x-small">6. PATIENT RELATIONSHIP TO INSURED
                                                        <div class="slds-grid slds-gutters check_row">
                                                            <div class="slds-col slds-size_4-of-6 slds-text-align_left">
                                                                <lightning:select aura:id="selectThree" name="{!allData.relationWithInsured + '$ElixirSuite__Patient_Relationship_With_Insured__c'}" label="{!allData.relationWithInsured}" value="{!allData.relationWithInsured}" onchange="{!c.radioCheckThree}" disabled="{!v.editAbility}">
                                                                    <aura:iteration items="{!v.relationshipTypePicklistValues}" var="optionItem">
                                                                        <option value="{!optionItem}" label="{!optionItem}" selected="{!allData.relationWithInsured == optionItem}"/>
                                                                    </aura:iteration>
                                                                </lightning:select>
                                                            </div>                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12 slds-m-bottom_x-small">8. RESERVED FOR NUCC USE
                                                        <lightning:helptext content="This field is reserved for NUCC use. The NUCC will provide instructions for any use of this field." class="customIcon"/>   
                                                        <lightning:textarea disabled="true" type="text"  value="{!allData.reserverdforNUCCuse1}"/>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small">7. INSURED'S ADDRESS(No., Street)
                                                        <lightning:textarea type="textarea" value="{!allData.insuredAddress}" maxlength="29"/>
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_6-of-12 slds-m-bottom_x-small">CITY
                                                        <lightning:input type="text" value="{!allData.insuredCity}" maxlength="23"/>
                                                    </div>
                                                    <div class="slds-col slds-size_6-of-12 slds-m-bottom_x-small">STATE
                                                        <lightning:input type="text" value="{!allData.insuredState}" maxlength="4"/>
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_6-of-12 slds-m-bottom_x-small">ZIP CODE
                                                        <lightning:input  pattern="[0-9]{5}" type="text" value="{!allData.insuredZipcode}" maxlength="5" name="{!idx1+'$'+idx}"
                                                                         aura:id="insuredzip"/>
                                                    </div>
                                                    <div class="slds-col slds-size_6-of-12 slds-m-bottom_x-small">TELEPHONE(Include Area Code)
                                                        <lightning:input type="text" value="{!allData.insuredTelephone}" maxlength="13"/>
                                                    </div>
                                                </div>
<div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small"> INSURED'S ADDRESS 2
                                                        <lightning:textarea type="textarea" value="{!allData.insuredAddress2}" maxlength="29"/>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="slds-grid slds-gutters">
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small">9. OTHER INSURED'S NAME (Last Name, First Name, Middle Name)
                                                        <lightning:helptext content="The “Other Insured’s Name” indicates that there is a holder of another policy that maycover the patient.Use commas to separate the last name, first name, and middle initial. A hyphen can be used for hyphenated names. Do not use periods within the name." class="customIcon"/>   
                                                        <lightning:input type="text" value="{!allData.otherInsuredName}"  maxlength="29" disabled="{!v.enableSecInsurance}"
                                                                         onblur="{!c.otherSame}"/>
                                                        <!--lightning:input type="text" value="{!v.commaSeparInsudName}"  maxlength="29" disabled="{!v.enableSecInsurance}"
                                                                         onblur="{!c.otherSame}"/-->
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small">a. OTHER INSURED’S POLICY OR GROUP NUMBER 
                                                        <lightning:input type="text" value="{!allData.otherInsuredPolicyNumber}" disabled="{!v.enableSecInsurance}"
                                                                         maxlength="28"/>
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small">b. RESERVED FOR NUCC USE 
                                                        <lightning:helptext content="This field is reserved for NUCC use. The NUCC will provide instructions for any use of this field." class="customIcon"/>   
                                                        <lightning:textarea type="text" value="{!allData.reserverdforNUCCuse2}" disabled="{!v.enableSecInsurance}"/>
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small">c. RESERVED FOR NUCC USE 
                                                        <lightning:helptext content="This field is reserved for NUCC use. The NUCC will provide instructions for any use of this field." class="customIcon"/>   
                                                        <lightning:textarea type="text" value="{!allData.reservedforNUCCuse3}" disabled="{!v.enableSecInsurance}"/>
                                                    </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small">d. INSURANCE PLAN NAME OR PROGRAM NAME 
                                                        <lightning:helptext content="The 'Insurance Plan Name or Program Name' identifies the name of the plan or program of the other insured as indicated in Item Number 9. " class="customIcon"/>   
                                                        <lightning:input type="text" value="{!allData.otherInsurancePlanName}"  disabled="{!v.enableSecInsurance}"
                                                                         maxlength="28"/>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="slds-col slds-size_4-of-12 slds-m-bottom_small">
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small">10. IS PATIENT'S CONDITION RELATED TO: </div>
                                                </div>
                                                <div class="slds-grid slds-gutters">
                                                    
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small slds-p-top_x-large">a. EMPLOYMENT? (Current or Previous)
                                                        <div class="slds-grid slds-gutters">
                                                            <aura:iteration items="{!v.employmentPicklistValues}" var="item">
                                                                <div class="slds-col slds-size_2-of-4 slds-text-align_left slds-p-left_medium slds-p-top_xx-small">
                                                                    <lightning:input type="checkbox" aura:id="checkFour"  name="{!item + '$ElixirSuite__Employment__c'}" 
                                                                                     label="{!item}" checked="{!v.isEmployement == item}" onchange="{!c.radioCheckFour}"/>
                                                                </div>
                                                            </aura:iteration>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!-- <lightning:outputField fieldName="autoAccident"  value="{!v.autoAccident}"/>-->
                                                <!--   <lightning:outputField fieldName="employment" />-->
                                                <!-- <lightning:input name="input5" label="Required date field"  value="{!c.autoAccident}" />-->
                                                <div class="slds-grid slds-gutters">
                                                    <div class="slds-col slds-size_12-of-12  slds-m-bottom_x-small slds-p-top_small">b. AUTO ACCIDENT?
                                                        <div class="slds-grid slds-gutters">
                                                            <aura:iteration items="{!v.autoAccidentPicklistValues}" var="item">
                                                                <div class="slds-col slds-size_2-of-4 slds-text-align_left slds-p-left_medium slds-p-top_xx-small">
