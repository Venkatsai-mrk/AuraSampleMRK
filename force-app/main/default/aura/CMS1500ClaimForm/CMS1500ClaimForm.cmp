                                                                <lightning:input  disabled="{!allData.isRowDisabled}" type="text" autocomplete="off" maxlength="8" name="{!rowIndex+'$'+childIdx}" messageWhenTooLong="4 sets of 2 characters each are allowed!" value="{!allData.modifier_Procedure3}" /> 
                                                                <lightning:input  disabled="{!allData.isRowDisabled}" type="text" autocomplete="off" maxlength="8" name="{!rowIndex+'$'+childIdx}" messageWhenTooLong="4 sets of 2 characters each are allowed!" value="{!allData.modifier_Procedure4}" /> 
                                                            </td>
                                                            <td>
