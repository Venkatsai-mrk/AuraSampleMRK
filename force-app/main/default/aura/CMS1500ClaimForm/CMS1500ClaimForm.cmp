                                                            </td>
                                                            <td>
                                                                <lightning:input  disabled="{!allData.isRowDisabled}" type="text" autocomplete="off" required="true" maxlength="4" name="{!rowIndex+'$'+childIdx}"  messageWhenTooLong="Only 4 characters of entry allowed!" value="{!allData.charges_Procedure}" onchange="{!c.reEvaluateTotalCharge}"/> 
                                                                
                                                            </td>
                                                            <td>
                                                                <lightning:input  disabled="{!allData.isRowDisabled}" type="text" autocomplete="off" required="true" maxlength="6" name="{!rowIndex+'$'+childIdx}" value="{!allData.daysOrUnit_Procedure}" onchange="{!c.reEvaluateTotalCharge}"/>
