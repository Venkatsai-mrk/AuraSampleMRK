                                <aura:if isTrue="{!v.plugData }">
                                    <lightning:button disabled="{!v.isAddClaimEnabled}" variant="brand" label="Save And Send to Insurance" name="Save And Send to Insurance" type="submit" onclick = "{!c.save}" /> <!--Anusha 02/11/22-->
                                </aura:if>
                            </div>
                        </footer>
                        
                    </div></div>
            </div>
            
        </div>
        
    </aura:if>
    <!-- Confirmation popup -->
    <aura:if isTrue="{!v.showConfirmation}">
        <div class="bottomsection">
            <div role="dialog" class="slds-modal slds-fade-in-open ">
                <div class="slds-modal__container main_modal_body">
                    <header class="slds-modal__header modal-header">
                        <h1 class="slds-text-heading--medium">Confirmation</h1>
                    </header>
                    <div class="slds-modal__content slds-p-around--medium modal_body">
                        <center style="font-weight: normal">{!v.errorMessage}</center>
                    </div>
                    <footer class="slds-modal__footer footer_btn">
                        <lightning:button variant="Neutral" name='No' label='No' onclick='{!c.handleNo}'/>
                        <lightning:button  variant="brand" name='Yes' label='Yes' onclick='{!c.handleYes}'/>
                    </footer>
                    
                </div>
            </div>
        </div>
        <div class="slds-backdrop slds-backdrop--open"></div> 
    </aura:if>
</aura:component>