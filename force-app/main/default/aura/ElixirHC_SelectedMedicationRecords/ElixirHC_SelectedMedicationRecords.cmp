                                                                                            <option value="">Choose one...</option>
                                                                                            <aura:iteration items="{!v.UnitsValues}" var="items2" >
                                                                                                
                                                                                                <option value="{!items2}" selected="{! items2 == itemInside.UnitsForPrn}">
                                                                                                    {!items2}
                                                                                                </option>
                                                                                            </aura:iteration>
                                                                                        </lightning:select>
                                                                                        
                                                                                    </div>
                                                                                </div>                                                                
                                                                            </div>                        
                                                                        </div>
                                                                        
                                                                    </aura:iteration>
                                                                </aura:if>
                                                            </aura:iteration>
                                                        </aura:if>
                                                        
                                                    </aura:if>
                                                    <aura:if isTrue="{!rec.types == 'Action Order'}">
                                                        
